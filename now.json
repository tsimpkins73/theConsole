{
    "version": 2,
    "name": "theconsole",
    "public": false,
    "builds": [
      { "src": "package.json", "use": "@now/static-build", 
    "config": {"distDir":"build"} }
    ],
    "routes": [
      { "src": "/manifest.json", "dest": "/manifest.json" },
      { "src": "^/static/(.*)", "dest": "/static/$1" },
      { "src": ".*", "dest": "/index.html" }
    ]
  }